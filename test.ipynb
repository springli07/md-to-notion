    "    def is_balanced(s):\n",
    "        single_dollar_count = s.count('$')\n",
    "        double_dollar_count = s.count('$$')\n",
    "\n",
    "        return single_dollar_count % 2 == 0 and double_dollar_count % 2 == 0"
